{
  "name": "template-electron-vue3",
  "version": "1.0.0",
  "description": "基于 Electron + Vue3 + TypeScript + Less + Vite 技术栈的项目模板",
  "main": "dist/electron/main.js",
  "repository": "git@github.com:NathanCong/template-electron-vue3.git",
  "author": "NathanCong <congyunan@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "electron-forge start",
    "clean": "rimraf dist out",
    "build:electron": "vite build --config vite.electron.config.ts",
    "build:preload": "vite build --config vite.preload.config.ts",
    "build:renderer": "vite build --config vite.renderer.config.ts",
    "package:win32": "electron-forge package --platform=win32",
    "package:linux": "electron-forge package --platform=linux",
    "make:win32": "electron-forge make --platform=win32",
    "make:linux": "electron-forge make --platform=linux",
    "make": "electron-forge make"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@types/less": "^3.0.8",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/tsconfig": "^0.7.0",
    "electron": "^36.4.0",
    "less": "^4.3.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
